{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-azuredproject"
		},
		"AzureDataLakeStorageGen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorageGen2'"
		},
		"AzureSqlDatabas_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSqlDatabas'"
		},
		"AzureDataLakeStorageGen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://mashekstorageproject.dfs.core.windows.net/"
		},
		"AzureSqlDatabas_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "azuresqlservermashek.database.windows.net"
		},
		"AzureSqlDatabas_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "mashekazuredqlone"
		},
		"AzureSqlDatabas_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "mashek"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-azuredproject_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorageGen2_accountKey": {
						"value": "[parameters('AzureDataLakeStorageGen2_accountKey')]"
					},
					"AzureSqlDatabas_password": {
						"value": "[parameters('AzureSqlDatabas_password')]"
					},
					"AzureDataLakeStorageGen2_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorageGen2_properties_typeProperties_url')]"
					},
					"AzureSqlDatabas_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlDatabas_properties_typeProperties_server')]"
					},
					"AzureSqlDatabas_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlDatabas_properties_typeProperties_database')]"
					},
					"AzureSqlDatabas_properties_typeProperties_userName": {
						"value": "[parameters('AzureSqlDatabas_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}